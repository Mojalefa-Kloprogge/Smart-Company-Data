            </connector>
            <label>No Branchnumber or Chamber of Commerce</label>
        </rules>
        <rules>
            <name>Has_Branchnumber</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Account.Branch_Number__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Has_Branch_Screen</targetReference>
            </connector>
            <label>Has Branchnumber</label>
        </rules>
        <rules>
            <name>Has_Chamber_of_Commerce</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Account.Chamber_of_Commerce__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Account.Branch_Number__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Account.Is_Branch__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Has_Chamber_of_Commerce_Screen</targetReference>
            </connector>
            <label>Has Chamber of Commerce</label>
        </rules>
    </decisions>
