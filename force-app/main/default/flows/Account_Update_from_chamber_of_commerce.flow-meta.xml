    <decisions>
        <description>This decision checks if a chamber of commerce number or a branch number is available</description>
        <name>Check_numbers</name>
        <label>Check numbers</label>
        <locationX>710</locationX>
        <locationY>278</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>No_Branchnumber_or_Chamber_of_Commerce</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Account.Branch_Number__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Account.Chamber_of_Commerce__c</leftValueReference>
